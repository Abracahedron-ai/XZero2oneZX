{
  "name": "zero2onez",
  "version": "1.0.0",
  "description": "Layered compositor with 3D avatar, live camera, and behavior brain",
  "main": "electron/main.js",
  "scripts": {
    "dev:renderer": "cd renderer && npm run dev",
    "dev:electron": "electron .",
    "dev:python": "cd python && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\" \"npm run dev:python\"",
    "build:renderer": "cd renderer && npm run build",
    "build": "npm run build:renderer && electron-builder",
    "start": "electron ."
  },
  "keywords": ["compositor", "3d", "avatar", "webrtc", "electron"],
  "author": "Zero2oneZ",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "ws": "^8.16.0"
  }
}
